<?xml version="1.0" encoding="UTF-8"?><sv:node xmlns:sv="http://www.jcp.org/jcr/sv/1.0" xmlns:h="http://www.onehippo.org/jcr/xmlimport" sv:name="cms" h:merge="combine">
  <sv:node sv:name="cms-services" h:merge="combine">
    <sv:node sv:name="cloudinaryService" h:merge="insert" h:location="galleryProcessorService">
      <sv:property sv:name="jcr:primaryType" sv:type="Name">
        <sv:value>frontend:plugin</sv:value>
      </sv:property>
      <sv:property sv:name="api_key" sv:type="String">
        <sv:value></sv:value>
      </sv:property>
      <sv:property sv:name="api_secret" sv:type="String">
        <sv:value></sv:value>
      </sv:property>
      <sv:property sv:name="cloud_name" sv:type="String">
        <sv:value></sv:value>
      </sv:property>
      <sv:property sv:name="plugin.class" sv:type="String">
        <sv:value>imageutil.plugin.CloudinaryService</sv:value>
      </sv:property>
    </sv:node>
    <sv:node sv:name="cloudinaryGalleryProcessorService">
      <sv:property sv:name="jcr:primaryType" sv:type="Name">
        <sv:value>frontend:plugin</sv:value>
      </sv:property>
      <sv:property sv:name="gallery.processor.id" sv:type="String">
        <sv:value>service.cloudinary.gallery.processor</sv:value>
      </sv:property>
      <sv:property sv:name="plugin.class" sv:type="String">
        <sv:value>imageutil.CloudinaryGalleryProcessorPlugin</sv:value>
      </sv:property>
      <sv:node sv:name="hippogallery:thumbnail">
        <sv:property sv:name="jcr:primaryType" sv:type="Name">
          <sv:value>frontend:pluginconfig</sv:value>
        </sv:property>
        <sv:property sv:name="crop" sv:type="String">
          <sv:value>thumb</sv:value>
        </sv:property>
        <sv:property sv:name="effect" sv:type="String">
          <sv:value>sepia</sv:value>
        </sv:property>
        <sv:property sv:name="gravity" sv:type="String">
          <sv:value>face</sv:value>
        </sv:property>
        <sv:property sv:name="height" sv:type="Long">
          <sv:value>60</sv:value>
        </sv:property>
        <sv:property sv:name="upscaling" sv:type="Boolean">
          <sv:value>false</sv:value>
        </sv:property>
        <sv:property sv:name="width" sv:type="Long">
          <sv:value>60</sv:value>
        </sv:property>
      </sv:node>
      <sv:node sv:name="hippogallery:original">
        <sv:property sv:name="jcr:primaryType" sv:type="Name">
          <sv:value>frontend:pluginconfig</sv:value>
        </sv:property>
        <sv:property sv:name="height" sv:type="Long">
          <sv:value>0</sv:value>
        </sv:property>
        <sv:property sv:name="upscaling" sv:type="Boolean">
          <sv:value>false</sv:value>
        </sv:property>
        <sv:property sv:name="width" sv:type="Long">
          <sv:value>0</sv:value>
        </sv:property>
      </sv:node>
    </sv:node>
  </sv:node>
</sv:node>
